class Solution {
    public int singleNumber(int[] nums) {

//by sorting O(Nlogn)
        int ans =0;
        Arrays.sort(nums);
        for(int i=0;i<nums.length-1;i=i+2){
            if(nums[i]!=nums[i+1]){
               return nums[i];
            }
        }
        return nums[nums.length-1];


//by XOR O(n)
        for(int i=0;i<nums.length;i++){
            ans^=nums[i];
        }
        return ans;

    }
}

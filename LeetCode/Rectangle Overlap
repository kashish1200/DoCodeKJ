class Solution {
    public boolean isRectangleOverlap(int[] rec1, int[] rec2) {
        int x1 = rec1[0], y1 = rec1[1], x2 = rec1[2], y2 = rec1[3];
        int X1 = rec2[0], Y1 = rec2[1], X2 = rec2[2], Y2 = rec2[3];
        if(x2 <= X1 || X2 <= x1 || y2 <= Y1 || Y2 <= y1){
            return false;
        }
        else return true;
    }
}
